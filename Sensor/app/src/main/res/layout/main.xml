<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="加速传感器返回值"
            android:textSize="20sp"/>
        <EditText
            android:id="@+id/acculerateTxt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="方向传感器返回值"
                android:textSize="20sp"/>
        <EditText
            android:id="@+id/orienationTxt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="磁场传感器返回值"
            android:textSize="20sp"/>
        <EditText
            android:id="@+id/magneticTxt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="温度传感器返回值"
            android:textSize="20sp"/>
        <EditText
            android:id="@+id/temperatorTxt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="压力传感器返回值"
            android:textSize="20sp"/>
        <EditText
            android:id="@+id/pressureTxt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
</LinearLayout>
